{
  "name": "gestion-eventos",
  "version": "0.0.1",
  "description": "Sistema de gestion de eventos",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:prisma": "npx prisma generate && nest start",
    "start:prisma:pull": "npx prisma db pull && npx prisma generate && nest start",
    "start:prisma:push": "npx prisma db push && npx prisma generate && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage --watchAll=false",
    "test:prisma": "npx prisma generate && jest --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "seed": "node --loader ts-node/esm src/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.14.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.18.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.6.0",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.5.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.10.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.8",
    "@types/node": "18.11.18",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.3.0",
    "prettier": "^2.3.2",
    "prisma": "^4.14.1",
    "sinon": "^15.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
